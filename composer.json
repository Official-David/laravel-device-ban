{
    "name": "david-adigwu/laravel-device-ban",
    "description": "A Laravel package to ban devices based on their IP address",
    "keywords": ["laravel", "device", "ban", "ip", "address"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Adigwu",
            "email": "david@davidadigwu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DavidAdigwu\\LaravelDeviceBan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidAdigwu\\LaravelDeviceBan\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidAdigwu\\LaravelDeviceBan\\LaravelDeviceBanServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-package-tools": "^1.92"
    }
}
